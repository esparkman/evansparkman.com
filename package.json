{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "env-cmd gatsby build",
    "develop": "yarn tailwind && env-cmd gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "yarn develop",
    "serve": "env-cmd gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "tailwind": "npx tailwind build src/styles/styles.scss -o src/styles/styles.css"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^13.4.0",
    "gatsby": "^2.23.20",
    "gatsby-plugin-postcss": "^2.3.9",
    "gatsby-plugin-sass": "^2.3.10",
    "gatsby-plugin-sharp": "^2.6.17",
    "gatsby-remark-images": "^3.3.17",
    "gatsby-remark-relative-images": "^0.3.0",
    "gatsby-source-contentful": "^2.3.22",
    "gatsby-source-filesystem": "^2.3.18",
    "gatsby-transformer-remark": "^2.8.23",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "typeface-montserrat": "^0.0.75",
    "typeface-open-sans": "^0.0.75"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "prettier": "^2.0.5",
    "tailwindcss": "^1.4.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
