{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "env-cmd gatsby build",
    "develop": "yarn tailwind && env-cmd gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "yarn develop",
    "serve": "env-cmd gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "tailwind": "npx tailwind build src/styles/styles.scss -o src/styles/styles.css"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^13.4.0",
    "gatsby": "^2.8.17",
    "gatsby-plugin-postcss": "^2.1.18",
    "gatsby-plugin-sass": "^2.0.26",
    "gatsby-plugin-sharp": "^2.3.10",
    "gatsby-remark-images": "^3.1.39",
    "gatsby-remark-relative-images": "^0.2.2",
    "gatsby-source-contentful": "^2.1.73",
    "gatsby-source-filesystem": "^2.1.43",
    "gatsby-transformer-remark": "^2.6.45",
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "sqreen": "^1.38.3",
    "typeface-montserrat": "^0.0.75",
    "typeface-open-sans": "^0.0.75"
  },
  "devDependencies": {
    "env-cmd": "^10.0.1",
    "prettier": "^1.19.1",
    "tailwindcss": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
